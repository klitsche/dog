{
    "directories": [
        "src"
    ],
    "finder": [
        {
            "name": "/LICENSE|LICENSE.md|license|license.md|.*\\.php/",
            "exclude": [
                "doc",
                "test",
                "tests",
                "Tests",
                "vendor-bin",
                "bin"
            ],
            "in": "vendor"
        }
    ],
    "compression": "GZ",
    "algorithm": "SHA256",
    "compactors": [
        "KevinGH\\Box\\Compactor\\Php",
        "KevinGH\\Box\\Compactor\\PhpScoper"
    ],
    "php-scoper": "box.php-scoper.php",
    "git-tag": "git-tag"
}
